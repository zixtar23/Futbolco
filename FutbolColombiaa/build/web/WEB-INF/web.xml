<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>Servelt.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <servlet-class>Servelt.RegistrarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>crearJugador</servlet-name>
        <servlet-class>Servelt.crearJugador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editarJugador</servlet-name>
        <servlet-class>Servelt.editarJugador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>niveles</servlet-name>
        <servlet-class>Servelt.niveles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editarCuenta</servlet-name>
        <servlet-class>Servelt.editarCuenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletUsuario</servlet-name>
        <servlet-class>controlador.ServletUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEquipo</servlet-name>
        <servlet-class>controlador.ServletEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletJugador</servlet-name>
        <servlet-class>controlador.ServletJugador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>controlador.NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <url-pattern>/nuevousuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crearJugador</servlet-name>
        <url-pattern>/crearJugador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editarJugador</servlet-name>
        <url-pattern>/editarJugador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>niveles</servlet-name>
        <url-pattern>/niveles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editarCuenta</servlet-name>
        <url-pattern>/editarCuenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletUsuario</servlet-name>
        <url-pattern>/ServletUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEquipo</servlet-name>
        <url-pattern>/ServletEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletJugador</servlet-name>
        <url-pattern>/ServletJugador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
